{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["plugin:vue/vue3-recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier"],
  "ignorePatterns": ["**/node_module/*", "**/build/*"],
  "rules": {
    "vue/multi-word-component-names": "off"
  }
}
